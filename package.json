{
  "name": "oeuf",
  "version": "0.1.19",
  "private": true,
  "homepage": "https://oeuf-fley.firebaseapp.com",
  "scripts": {
    "build": "npm-run-all build-css build-js",
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "build-js": "react-scripts build && cra-append-sw ./custom-sw.js",
    "eject": "react-scripts eject",
    "start": "npm-run-all -p watch-css start-js",
    "start-js": "react-scripts start && cra-append-sw --mode dev ./custom-sw.js",
    "storybook": "npm-run-all -p watch-css storybook-js",
    "storybook-js": "start-storybook -p 9001 -c .storybook",
    "test": "react-scripts test",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "lint": "tslint --project tsconfig.json 'src/**/*.{ts,tsx}'"
  },
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.8",
    "@fortawesome/fontawesome-svg-core": "^1.2.15",
    "@fortawesome/free-regular-svg-icons": "^5.7.2",
    "@fortawesome/free-solid-svg-icons": "^5.7.2",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "bootstrap": "^4.3.1",
    "idb": "^3.0.2",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-redux": "^6.0.1",
    "react-router-dom": "^4.3.1",
    "react-sortable-hoc": "^1.6.1",
    "react-swipeable-views": "^0.13.1",
    "redux": "^4.0.1",
    "redux-saga": "^1.0.2"
  },
  "devDependencies": {
    "@sambego/storybook-state": "^1.3.3",
    "@storybook/addon-a11y": "^5.0.0",
    "@storybook/addon-actions": "^5.0.0",
    "@storybook/addon-viewport": "^5.0.0",
    "@storybook/react": "^5.0.0",
    "@types/classnames": "^2.2.7",
    "@types/jest": "^24.0.9",
    "@types/react": "^16.8.6",
    "@types/react-dom": "^16.8.2",
    "@types/react-redux": "^7.0.1",
    "@types/react-router-dom": "^4.3.1",
    "@types/react-sortable-hoc": "^0.6.5",
    "@types/react-swipeable-views": "^0.13.0",
    "@types/redux-logger": "^3.0.7",
    "@types/storybook__addon-a11y": "^3.3.1",
    "@types/storybook__addon-actions": "^3.4.2",
    "@types/storybook__react": "^4.0.1",
    "@types/uuid": "^3.4.4",
    "babel-core": "^7.0.0-bridge.0",
    "cra-append-sw": "^2.6.1",
    "eslint-plugin-import": "^2.16.0",
    "jest": "^24.1.0",
    "node-sass-chokidar": "^1.3.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.16.4",
    "react-scripts": "^2.1.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-logger": "^3.0.6",
    "tslint": "^5.13.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^3.6.0",
    "typescript": "^3.3.3333"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
