{
  "name": "oeuf",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://fley.github.io/oeuf",
  "scripts": {
    "build": "npm-run-all build-css build-js",
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "build-js": "react-scripts build",
    "eject": "react-scripts eject",
    "start": "npm-run-all -p watch-css start-js",
    "start-js": "react-scripts start",
    "storybook": "npm-run-all -p watch-css storybook-js",
    "storybook-js": "start-storybook -p 9001 -c .storybook",
    "test": "react-scripts test",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "lint": "tslint --project tsconfig.json 'src/**/*.{ts,tsx}'"
  },
  "dependencies": {
    "@craco/craco": "^3.2.3",
    "@fortawesome/fontawesome": "^1.1.8",
    "@fortawesome/fontawesome-svg-core": "^1.2.12",
    "@fortawesome/free-regular-svg-icons": "^5.6.3",
    "@fortawesome/free-solid-svg-icons": "^5.6.3",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "bootstrap": "^4.2.1",
    "idb": "^2.1.3",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-redux": "^5.1.1",
    "react-router-dom": "^4.3.1",
    "react-sortable-hoc": "^0.6.8",
    "react-swipeable-views": "^0.12.18",
    "redux": "^4.0.1",
    "redux-saga": "^0.16.2",
    "redux-thunk": "^2.3.0",
    "typescript": "^3.2.2"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^4.1.3",
    "@storybook/addon-actions": "^4.1.3",
    "@storybook/addon-viewport": "^4.1.3",
    "@storybook/react": "^4.1.3",
    "@types/classnames": "^2.2.6",
    "@types/jest": "^23.3.10",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "@types/react-redux": "^6.0.11",
    "@types/react-router-dom": "^4.3.1",
    "@types/react-sortable-hoc": "^0.6.4",
    "@types/react-swipeable-views": "^0.12.2",
    "@types/redux-logger": "^3.0.6",
    "@types/storybook__addon-a11y": "^3.3.1",
    "@types/storybook__addon-actions": "^3.4.1",
    "@types/storybook__react": "^4.0.0",
    "@types/uuid": "^3.4.4",
    "awesome-typescript-loader": "^5.2.1",
    "babel-core": "^7.0.0-bridge.0",
    "eslint-plugin-import": "^2.14.0",
    "jest": "^23.6.0",
    "node-sass-chokidar": "^1.3.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.15.3",
    "react-scripts": "^2.1.2",
    "redux-devtools-extension": "^2.13.7",
    "redux-logger": "^3.0.6",
    "tslint": "^5.12.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^3.6.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
